    <main>
        <section>
            <h2>Book Your Package</h2>
            <form id="booking-form" novalidate>
                <label for="name">Name:</label>
                <input type="text" id="name" name="name" required>
                <div id="name-error" class="error-message">Please enter your name.</div>

                <label for="email">Email:</label>
                <input type="email" id="email" name="email" required>
                <div id="email-error" class="error-message">Please enter a valid email address.</div>

                <label for="package">Package Name:</label>
                <select id="package" name="package" required>
                    <option value="" disabled selected>Select a package</option>
                    <option value="tropical-paradise-getaway">Tropical Paradise Getaway</option>
                    <option value="cultural-city-experience">Cultural City Experience</option>
                    <option value="mountain-adventure-retreat">Mountain Adventure Retreat</option>
                    <option value="safari-adventure">Safari Adventure</option>
                    <option value="relaxing-beach-vacation">Relaxing Beach Vacation</option>
                    <option value="historic-tour">Historic Tour</option>
                </select>
                <div id="package-error" class="error-message">Please select a package.</div>

                <label for="mobile">Mobile Number:</label>
                <input type="tel" id="mobile" name="mobile" required>
                <div id="mobile-error" class="error-message">Please enter a valid mobile number.</div>

                <label for="guests">Number of Guests:</label>
                <input type="number" id="guests" name="guests" min="1" required>
                <div id="guests-error" class="error-message">Please enter a valid number of guests.</div>

                <button type="submit">Book Now</button>
            </form>
            <div id="order-confirmation">
                <p>Thank you, Lekshmi Nair! Your booking has been received. We will contact you shortly.</p>
            </div>
        </section>
    </main>
    <footer>
        <p>© 2025 Travel Agency Home. All rights reserved.</p>
    </footer>

    <script>
        const form = document.getElementById('booking-form');
        const orderConfirmation = document.getElementById('order-confirmation');

        function validateName() {
            const nameInput = document.getElementById('name');
            const nameError = document.getElementById('name-error');
            if (!nameInput.value.trim()) {
                nameInput.classList.add('error-border');
                nameError.style.display = 'block';
                return false;
            } else {
                nameInput.classList.remove('error-border');
                nameError.style.display = 'none';
                return true;
            }
        }

        function validateEmail() {
            const emailInput = document.getElementById('email');
            const emailError = document.getElementById('email-error');
            const emailRegex = /^[\w-]+(\.[\w-]+)*@([\w-]+\.)+[a-zA-Z]{2,7}$/;
            if (!emailRegex.test(emailInput.value.trim())) {
                emailInput.classList.add('error-border');
                emailError.style.display = 'block';
                return false;
            } else {
                emailInput.classList.remove('error-border');
                emailError.style.display = 'none';
                return true;
            }
        }

        function validatePackage() {
            const packageInput = document.getElementById('package');
            const packageError = document.getElementById('package-error');
            if (!packageInput.value) {
                packageInput.classList.add('error-border');
                packageError.style.display = 'block';
                return false;
            } else {
                packageInput.classList.remove('error-border');
                packageError.style.display = 'none';
                return true;
            }
        }

        function validateMobile() {
            const mobileInput = document.getElementById('mobile');
            const mobileError = document.getElementById('mobile-error');
            const mobileRegex = /^[0-9]{10}$/;
            if (!mobileRegex.test(mobileInput.value.trim())) {
                mobileInput.classList.add('error-border');
                mobileError.style.display = 'block';
                return false;
            } else {
                mobileInput.classList.remove('error-border');
                mobileError.style.display = 'none';
                return true;
            }
        }

        function validateGuests() {
            const guestsInput = document.getElementById('guests');
            const guestsError = document.getElementById('guests-error');
            if (!guestsInput.value || parseInt(guestsInput.value) < 1) {
                guestsInput.classList.add('error-border');
                guestsError.style.display = 'block';
                return false;
            } else {
                guestsInput.classList.remove('error-border');
                guestsError.style.display = 'none';
                return true;
            }
        }

        form.addEventListener('submit', (event) => {
            event.preventDefault();
            let isFormValid = true;

            isFormValid = validateName() && isFormValid;
            isFormValid = validateEmail() && isFormValid;
            isFormValid = validatePackage() && isFormValid;
            isFormValid = validateMobile() && isFormValid;
            isFormValid = validateGuests() && isFormValid;

            if (isFormValid) {
                orderConfirmation.style.display = 'block';
                form.reset();
                 setTimeout(() => {
                    orderConfirmation.style.display = 'none';
                }, 3000);
            }
        });

        document.getElementById('name').addEventListener('input', validateName);
        document.getElementById('email').addEventListener('input', validateEmail);
        document.getElementById('package').addEventListener('change', validatePackage);
        document.getElementById('mobile').addEventListener('input', validateMobile);
        document.getElementById('guests').addEventListener('input', validateGuests);
    </script>
</body>
</html>
