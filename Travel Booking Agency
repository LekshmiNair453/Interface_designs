    <script>
        let cart = [];

        document.addEventListener("DOMContentLoaded", () => {
            // Retrieve the cart from localStorage
            const storedCart = localStorage.getItem("travelAgencyCart");
            if (storedCart) {
                cart = JSON.parse(storedCart);
            }
            updateCart();
        });

        function updateCart() {
            const cartItemsElement = document.getElementById("cart-items");
            let cartContent = "";
            let total = 0;

            if (cart.length === 0) {
                cartContent += "<li>Your cart is empty</li>";
            } else {
                cart.forEach(item => {
                    cartContent += `<li><span>${item.name}</span><span>$${item.price}</span></li>`;
                    total += item.price;
                });
            }

            cartItemsElement.innerHTML = cartContent;

            const cartTotalElement = document.getElementById("cart-total");
            cartTotalElement.textContent = `Total: $${total}`;

             // Store the cart in localStorage
            localStorage.setItem("travelAgencyCart", JSON.stringify(cart));
        }

        function checkout() {
            if (cart.length === 0) {
                alert("Your cart is empty. Please add items to your cart before checking out.");
                return;
            }
            const modal = document.getElementById("buy-now-modal");
            modal.style.display = "flex";
        }

        function placeOrder() {
            const email = document.getElementById("email").value;
            const mobile = document.getElementById("mobile").value;
            const address = document.getElementById("address").value;

            if (email && mobile && address) {
                alert("Your order has been placed!");
                cart = [];
                updateCart();
                localStorage.removeItem("travelAgencyCart");
                document.getElementById("buy-now-modal").style.display = "none";
                window.location.href="index.html";
            } else {
                alert("Please enter your email, mobile number, and address.");
            }
        }

        function closeModal() {
            document.getElementById("buy-now-modal").style.display = "none";
        }
    </script>
</head>
<body>
    <div class="header">
        <div>Travel Agency Home</div>
        <div class="nav">
            <a href="index.html">Culinary Packages</a>
            <a href="packages.html">Packages</a>
            <a href="shop.html">Shop</a>
            <a href="about.html">About</a>
            <a href="cart.html">Cart</a>
            <a href="bookings.html">Bookings</a>
        </div>
    </div>
    <div class="cart-content">
        <h1>Your Cart</h1>
        <ul class="cart-items" id="cart-items">
            <li>Your cart is empty</li>
        </ul>
        <p class="cart-total" id="cart-total">Total: $0</p>
        <button class="buy-now-button" onclick="checkout()">Checkout</button>
        <div class="buy-now-modal" id="buy-now-modal">
            <div class="buy-now-modal-content">
                <button class="close-modal-button" onclick="closeModal()">&times;</button>
                <h2>Enter Your Details</h2>
                <label for="email">Email:</label>
                <input type="email" id="email" name="email" required>
                <label for="mobile">Mobile Number:</label>
                <input type="tel" id="mobile" name="mobile" required>
                <label for="address">Address:</label>
                <input type="text" id="address" name="address" required>
                <button onclick="placeOrder()">Place Order</button>
            </div>
        </div>
    </div>
</body>
</html>



