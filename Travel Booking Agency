    <script>
        function showBookingForm(packageName, price) {
            const modal = document.getElementById("bookingModal");
            modal.style.display = "block";

            document.getElementById("modalPackageName").value = packageName;
            document.getElementById("modalPrice").value = price;
            document.getElementById("modalTitle").innerHTML = `Book Your ${packageName} Tour`;
        }

        function closeModal() {
            const modal = document.getElementById("bookingModal");
            modal.style.display = "none";
        }

        function submitBooking() {
            const name = document.getElementById("modalName").value;
            const email = document.getElementById("modalEmail").value;
            const phone = document.getElementById("modalPhone").value;
            const guests = document.getElementById("modalGuests").value;
            const message = document.getElementById("modalMessage").value;

            if (name && email && phone && guests) {
                alert(`Thank you, ${name}! Your booking has been received. We will contact you shortly.`);
                closeModal();
                 window.location.href="index.html";

            } else {
                alert("Please fill in all the required fields.");
            }
        }
    </script>
</head>
